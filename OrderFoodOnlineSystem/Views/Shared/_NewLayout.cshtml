<body>
    <!-- Navigation -->
    <div class="header">
        <header class="full-width">
            
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12 mainNavCol">
                        <!-- logo -->
                        <a asp-action="Index" asp-controller="Home">
                            <img src="~/assets/img/icons/home2.png" class="img-fluid" alt="Logo">
                        </a>
                        @* <div class="logo mainNavCol"> *@
                        @*     <a asp-action="Index" asp-controller="Home"> *@
                        @*         <img src="~/assets/img/logo.png" class="img-fluid" alt="Logo"> *@
                        @*     </a> *@
                        @* </div> *@

                        <div class="logo mainNavCol">
                            <a asp-action="Index" asp-controller="Home">
                                @* <img src="~/assets/img/OurLogo.jpg" class="img-fluid" alt="Logo"> *@
                                <img src="~/assets/img/fianl logo.jpg" class="img-fluid" alt="Logo" style="max-width:7vw">
                            </a>
                        </div>



                        <!-- logo -->
                        <div class="main-search mainNavCol d-flex justify-content-center">

                            <form asp-action="GetAll" asp-controller="Restaurant" class="main-search search-form full-width">
                                <div class="row justify-content-center">
                                    <div class="col-lg-6 col-md-7">
                                        <div class="search-box padding-10 d-flex align-items-center">
                                            <input type="text" name="restaurantName" value="@ViewBag.restaurantName"
                                                   class="form-control me-2"
                                                   placeholder="Search by restaurant name...">

                                            <a asp-action="GetAll" asp-controller="Restaurant" class="btn btn-outline-danger fw-bold ms-2 px-4 py-2" style="transition: box-shadow 0.3s ease, letter-spacing 0.2s ease, background-color 0.3s ease, color 0.3s ease;" onmouseover="this.style.boxShadow='0px 4px 15px rgba(0, 0, 0, 0.1)'; this.style.letterSpacing='1px'; this.style.backgroundColor='transparent'; this.style.color='red';" onmouseout="this.style.boxShadow='none'; this.style.letterSpacing='0'; this.style.backgroundColor='transparent'; this.style.color='red';">Clear</a>
                                        </div>
                                    </div>
                                </div>
                            </form>

                        </div>
                        <div class="right-side fw-700 mainNavCol">
                            <div class="gem-points">
                            </div>
                            <!-- mobile search -->
                            <div class="mobile-search p-3">
                                <a href="#" data-toggle="modal" data-target="#search-box">
                                    <i class="fas fa-search"></i>
                                </a> 
                            </div> 
                            <!-- mobile search -->
                            <!-- user account -->
                            @if (!User.Identity.IsAuthenticated)
                            {
                                <div class=" p-relative right-side fw-700 mainNavCol"
                                     style="display: flex; gap: 10px; align-items: center; justify-content: center;">
                                    <a class="text-light-green fw-700" asp-controller="Account" asp-action="Login">
                                        Sign
                                        in
                                    </a>
                                    <a class="text-light-green fw-700" asp-controller="Account" asp-action="Register">
                                        Sign
                                        up
                                    </a>
                                </div>
                            }
                            
                            else
                            {
                                <div class="user-details p-relative">

                                    <a href="#" class="text-light-white fw-500">
                                        <img src="~/assets/img/icons/account-3.png" class="rounded-circle" alt="userimg"> <span>
                                            Hi,
                                            @User.FindFirst("FName")?.Value
                                        </span>
                                    </a>
                                    <div class="user-dropdown">
                                        <ul>
                                            <li>
                                                <a asp-action="Edit" asp-controller="Customer">
                                                    <div class="icon">
                                                        <i class="flaticon-user"></i>
                                                    </div> <span class="details">Account</span>
                                                </a>
                                            </li>

                                            <li>
                                                <a asp-action="GetAllCustomerOrders" asp-controller="Order">
                                                    <div class="icon">
                                                        <i class="flaticon-rewind"></i>
                                                    </div> <span class="details">Your Orders</span>
                                                </a>
                                            </li>

                                            <li>
                                                <a asp-action="GetAllFavourites" asp-controller="FavouriteRestaurant">
                                                    <div class="icon">
                                                        <i class="flaticon-diamond"></i>
                                                    </div> <span class="details">Favorite Restaurants</span>
                                                </a>
                                            </li>

                                            <li>
                                                <a asp-action="Index" asp-controller="Address">
                                                    <div class="icon">
                                                        <i class="bi-house-door"></i>
                                                    </div> <span class="details">Saved Addresses</span>
                                                </a>
                                            </li>

                                            <li class="mt-2">
                                                <a asp-action="Index" asp-controller="Review">
                                                    <div class="icon">
                                                        <i class="bi-chat-square-text"></i>
                                                    </div> <span class="details">Your Reviews</span>
                                                </a>
                                            </li>

                                        </ul>
                                        <div class="user-footer">
                                            <a class="nav-link text-dark" asp-controller="Account" asp-action="Logout">Sign Out</a>
                                        </div>
                                    </div>
                                </div>
                            }
                            
                            @* Cart *@
                            @await Component.InvokeAsync("Cart")

                        
                        </div>
                        <!-- user cart -->
                    </div>
                </div>

            </div>
    
        
        </header>
    </div>


@RenderBody()


